<div id="reportsChart"></div>

<script>
    document.addEventListener("DOMContentLoaded", () => {
        const completedLearningPaths = 30; // Total number of completed learning paths
        const days = 7; // Total number of days

        // Generate a random distribution of learning paths across days
        const dataSeries = Array(days).fill(0);
        let remainingLearningPaths = completedLearningPaths;
        for (let i = 0; i < days - 1; i++) {
            const maxPathsPerDay = Math.ceil(remainingLearningPaths / (days - i));
            const randomPathsToday = Math.floor(Math.random() * (maxPathsPerDay + 1)); // Random number of paths for today
            dataSeries[i] = randomPathsToday;
            remainingLearningPaths -= randomPathsToday;
        }
        dataSeries[days - 1] = remainingLearningPaths; // Assign the remaining paths to the last day

        new ApexCharts(document.querySelector("#reportsChart"), {
            series: [{
                name: 'Learning Paths',
                data: dataSeries,
            }],
            chart: {
                height: 350,
                type: 'area',
                toolbar: {
                    show: false
                },
            },
            markers: {
                size: 4
            },
            colors: ['#4154f1'],
            fill: {
                type: "gradient",
                gradient: {
                    shadeIntensity: 1,
                    opacityFrom: 0.3,
                    opacityTo: 0.4,
                    stops: [0, 90, 100]
                }
            },
            dataLabels: {
                enabled: false
            },
            stroke: {
                curve: 'smooth',
                width: 2
            },
            xaxis: {
                type: 'datetime',
                categories: ["2023-07-16T00:00:00.000Z", "2023-07-17T00:00:00.000Z", "2023-07-18T00:00:00.000Z", "2023-07-19T00:00:00.000Z", "2023-07-20T00:00:00.000Z", "2023-07-21T00:00:00.000Z", "2023-07-22T00:00:00.000Z"] // Dates of Learning Paths completion
            },
            tooltip: {
                x: {
                    format: 'dd/MM/yy HH:mm'
                },
            }
        }).render();
    });
</script>





